        "@babel/core",
        "@babel/plugin-transform-react-jsx",
        "vite",
        "react",
        "react-dom",
        "@types/react",
        "tslib",
        "quill-delta",
        "rxjs"
      ],
      "allowAny": [
        "rustikus"
      ]
    }
  },
