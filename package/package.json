{
  "name": "@bgunnarsson/toaster",
  "version": "0.0.10",
  "description": "Toast message javascript library",
  "type": "module",
  "access": "private",
  "main": "dist/toaster.cjs.js",
  "module": "dist/toaster.esm.js",
  "scripts": {
    "build": "rollup -c",
    "dist": "pnpm publish"
  },
  "exports": {
    ".": {
      "import": "./dist/toaster.esm.js",
      "require": "./dist/toaster.cjs.js",
      "types": "./toaster.d.ts"
    },
    "./react": {
      "import": "./dist/react/index.js"
    }
  },
  "types": "./toaster.d.ts",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "toaster-logo.png",
    "toaster.d.ts"
  ],
  "keywords": [
    "toast",
    "messages",
    "toast messages"
  ],
  "author": "B. Gunnarsson | www.bgunnarsson.com",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "rollup": "^4.24.0"
  }
}